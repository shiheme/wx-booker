/**app.wxss**/

.wrapper-whitebg {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(255, 255, 255, 1);
  z-index: -1;
}

.wrapper-blackbg {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(20, 20, 20, 1);
  z-index: -1;
}

.wrapperhide {
  opacity: 0 !important;
}

.container {
  box-sizing: border-box;
  padding-bottom: calc(80rpx + env(safe-area-inset-bottom));
  position: relative;
  z-index: 1;
}

.cntlistbox {
  padding-top: 40rpx;
}

.imtit {
  position: relative;
  margin: 50rpx 50rpx 0 50rpx;
}

.imtit-label {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.imtit-label-great {
  justify-content: left;
  text-align: left;
}

.imtit-label-title {
}

.imtit-label-title .maintit {
  font-size: 44rpx;
  font-weight: bold;
  display: block;
}

.imtit-label-great .imtit-label-title .maintit {
  font-size: 50rpx;
  font-weight: bold;
  display: block;
}

.imtit-label-title .subtit {
  display: block;
  font-weight: bold;
  font-size: 30rpx;
  margin: 20rpx 0;
}

.card {
  position: relative;
  margin: 40rpx 50rpx 0rpx 50rpx;
  border-radius: 16rpx;
  box-shadow: 2px 2px 20px rgba(0, 0, 0, 0.1);
  overflow: hidden;
  z-index: 10;
}

.card .cardbody {
  overflow: hidden;
}

.card .cardcell {
  height: 100rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.card .cardcell:last-child {
  border-bottom: none !important;
}

.cardcell .tit {
  font-size: 28rpx;
  font-weight: bold;
}

.cardcell .titnor {
  font-weight: normal;
}

.cardcell .tit .txt {
  font-size: 24rpx;
  font-weight: normal;
  margin-left: 20rpx;
}

.cardcell .more {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.cardcell .more .num {
  font-weight: bold;
  font-size: 26rpx;
}

.cardcell .more image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}

.cardcell .more .txt {
  font-size: 24rpx;
}

.cardcell .more icon {
  align-self: center;
  margin-left: 20rpx;
}

.cardcell .more icon:after {
  content: '';
  display: block;
  width: 10rpx;
  height: 10rpx;
  transform: rotate(45deg);
}

.loadingmore {
  margin: 40rpx 50rpx;
  text-align: center;
}

.loadingmore .no-more {
  line-height: 50rpx;
  font-size: 24rpx;
}

/*书本叠加样式*/

image.cover:before {
  content: '';
  position: absolute;
  z-index: 10;
  width: 100%;
  height: 100%;
  opacity: 0.5;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: url("data:image/png;base64,");
  background-position: 0 0;
  background-repeat: repeat-y;
  background-size: contain;
}

/* 登录框 */

.login-popup {
  position: fixed;
  left: 0;
  bottom: -100%;
  width: 100%;
  height: 100%;
  z-index: 999;
}

.popup-show {
  z-index: 999;
  bottom: 0;
}

.login-popup .popup-mask {
  position: relative;
  top: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  z-index: 5;
  background: rgba(0, 0, 0, 0.5);
}

.login-popup .popup-container {
  position: absolute;
  bottom: -450rpx;
  height: 450rpx;
  width: 100%;
  z-index: 10;
  background: rgba(255, 255, 255, 1);
  border-radius: 40rpx 40rpx 0 0;
  transition: 0.2s;
}

.login-popup.popup-show .popup-container {
  bottom: 0rpx;
  transition: 0.2s;
}

.login-popup-wrapper {
  padding: 50rpx;
}

.login-popup-wrapper .tipcnt {
  font-size: 32rpx;
  display: flex;
  align-items: center;
}

.login-popup-wrapper .tipcnt icon {
  margin-right: 10rpx;
}

.login-popup-wrapper .tipbtn {
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}

.login-popup-wrapper .tipbtn button {
  flex: auto;
  border-radius: 20rpx;
  height: 60rpx;
}

.login-popup-wrapper .tipbtn button.login-btn {
  background: #13c471;
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
}

.login-popup-wrapper .tipbtn button.reset {
  background: #f0f0f0;
  color: #333;
  font-size: 32rpx;
}

.login-popup-wrapper .tipimportant {
  color: rgba(0, 0, 0, 0.8);
  font-weight: bold;
  font-size: 24rpx;
  margin-top: 30rpx;
}

.showerror {
  margin: 50rpx;
}

.showerror image {
  width: 100rpx;
  height: 100rpx;
}

.showerror .errortext {
  font-size: 32rpx;
}

.whitebg showerror .errortext {
  color: rgba(0, 0, 0, 0.8);
}

.blackbg showerror .errortext {
  color: rgba(255, 255, 255, 0.8);
}

.wxParse-p {
  margin: 15rpx 0;
}

.wxParse-blockquote {
  padding: 20rpx !important;
}

.floatads {
  position: fixed;
  right: -200rpx;
  bottom: 10rpx;
  transition: 0.5s;
  z-index: 100;
  opacity: 1;
  padding-bottom: calc(0rpx + env(safe-area-inset-bottom));
}

.flb_open_imptbtn .floatads {
  margin-bottom: calc(112rpx + env(safe-area-inset-bottom));
}

.flb_open_dtnavads .floatads {
  margin-bottom: calc(130rpx + env(safe-area-inset-bottom));
}

.flb_open_imptbtn.flb_open_dtnavads .floatads {
  margin-bottom: calc(250rpx + env(safe-area-inset-bottom));
}

.floatads.active {
  right: 30rpx;
  transition: 0.5s;
}

.floatads image {
  width: 150rpx;
  animation: music-move 1s 0.5s infinite alternate ease-in-out;
  transform-origin: bottom;
  opacity: 1;
}

.floatads image.transactive {
  opacity: 0.3;
}

@keyframes grow {
  0% {
    transform: scale(0);
  }

  60% {
    transform: scale(1.15);
  }

  80% {
    transform: scale(0.95);
  }

  100% {
    transform: scale(1);
  }
}

@keyframes music-move {
  0% {
    transform: rotate(0deg);
  }

  33% {
    transform: rotate(-5deg);
  }

  66% {
    transform: rotate(5deg);
  }

  100% {
    transform: rotate(0deg);
  }
}

.container.whitebg .card {
  background: rgba(255, 255, 255, 1);
}

.container.whitebg .wxParse-blockquote {
  background: rgba(245, 245, 245, 1);
}

.container.whitebg, .container.whitebg .imtit-label-title .maintit,
.container.whitebg .cardcell .tit, .container.whitebg .cardcell .more .num {
  color: rgba(0, 0, 0, 0.8);
}

.container.whitebg .imtit-label-title .subtit,
.container.whitebg .cardcell .more .txt, .container.whitebg .cardcell .tit .txt {
  color: rgba(0, 0, 0, 0.6);
}

.container.whitebg .cardcell .more icon:after {
  border-right: 2rpx solid rgba(0, 0, 0, 0.5);
  border-top: 2rpx solid rgba(0, 0, 0, 0.5);
}

.container.whitebg .card .cardcell {
  border-bottom: 1px solid rgba(0, 0, 0, 0.05);
}
.container.whitebg .booklist .cardfoot .case {
  border-color: rgba(0, 0, 0, 0.8);
}
.container.whitebg .loadingmore .no-more {
  color: rgba(0, 0, 0, 0.4);
}


.container.blackbg .card {
  background: rgba(30, 30, 30, 1);
}

.container.blackbg .wxParse-blockquote {
  background: rgba(30, 30, 30, 1);
}

.container.blackbg, .container.blackbg .imtit-label-title .maintit,
.container.blackbg .cardcell .tit, .container.blackbg .cardcell .more .num {
  color: rgba(255, 255, 255, 0.8);
}

.container.blackbg .imtit-label-title .subtit,
.container.blackbg .cardcell .tit .txt, .container.blackbg .cardcell .more .txt {
  color: rgba(255, 255, 255, 0.6);
}

.container.blackbg .cardcell .more icon:after {
  border-right: 2rpx solid rgba(255, 255, 255, 0.5);
  border-top: 2rpx solid rgba(255, 255, 255, 0.5);
}

.container.blackbg .card .cardcell {
  border-bottom: 1px solid rgba(255, 255, 255, 0.05);
}


.container.blackbg .booklist .cardfoot .case {
  border-color: rgba(255, 255, 255, 0.8);
}

.container.blackbg .loadingmore .no-more {
  color: rgba(255, 255, 255, 0.4);
}

.wxParse-videolook {
  margin: 30rpx 0;
}

.wxParse-hidecnt {
  padding: 15rpx 30rpx;
}

.wxParse-hidecnt .wxParse-ul {
  margin-top: 30rpx;
  font-weight: bold;
}

.wxParse-hidecnt .wxParse-ul .wxParse-li,
.wxParse-hidecnt .wxParse-ul .wxParse-li .wxParse-li-inner {
  margin: 0rpx;
}

.wxParse-hidecnt .wxParse-ul .wxParse-li .wxParse-li-circle {
  display: none;
}

.wxParse-fileopen {
  display: flex;
  align-items: center;
}

.wxParse-fileopen .wxParse-img {
  width: 50rpx;
  height: 50rpx;
}

.wxParse-fileopen .wxParse-tit .wxParse-tit-limit text {
  width: 300rpx;
  font-weight: bold;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  color: deepskyblue;
}

.wxParse-hidecntbtn {
  padding: 30rpx;
  display: flex;
  align-items: center;
  flex-flow: column;
  text-align: center;
}

.wxParse-hidecntbtn .wxParse-img {
  width: 60rpx;
  height: 60rpx;
}

.wxParse-hidecntbtn .wxParse-tit {
  width: 40%;
}
